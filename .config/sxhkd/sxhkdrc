	showtray.sh
XF86AudioLowerVolume
	volume.sh down && pkill -RTMIN+10 $BAR
XF86AudioRaiseVolume
	volume.sh up && pkill -RTMIN+10 $BAR
XF86AudioMute
	volume.sh mute && pkill -RTMIN+10 $BAR
XF86MonBrightnessUp
	xbacklight -inc 5 && pkill -RTMIN+11 $BAR
XF86MonBrightnessDown
	xbacklight -dec 5 && pkill -RTMIN+11 $BAR
