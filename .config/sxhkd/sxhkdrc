	$TERMINAL -e youtube-download.sh
super + control + d
	display-select.sh
super + F3
	dmenumount
super + F4
	dmenuumount
super + a
	copytext
super + shift + a
	deletext
super + c
	select.sh $HOME/.local/share/copytext
super + shift + p
	select.sh $HOME/.local/share/i3/paths
super + shift + c
	mpv --title "webcam" --untimed --no-correct-pts --no-cache --cache-secs=0 --demuxer-readahead-secs=0 av://v4l2:/dev/video0
super + control + c
	pkill mpvfloat
super + t
	showtray.sh
XF86AudioLowerVolume
	volume.sh down && pkill -RTMIN+10 $BAR
XF86AudioRaiseVolume
	volume.sh up && pkill -RTMIN+10 $BAR
XF86AudioMute
	volume.sh mute && pkill -RTMIN+10 $BAR
XF86MonBrightnessUp
	xbacklight -inc 10 && pkill -RTMIN+11 $BAR
XF86MonBrightnessDown
	xbacklight -dec 10 && pkill -RTMIN+11 $BAR
super + g
	mpc toggle ; pkill -RTMIN+12 $BAR
super + s
	mpc stop ; pkill -RTMIN+12 $BAR
super + n
	mpc next ; pkill -RTMIN+12 $BAR
super + shift + n
	mpc prev ; pkill -RTMIN+12 $BAR
super + u
	mpd_update
super + shift + l
	mpd_load.sh ; pkill -RTMIN+12 $BAR
Caps_Lock
	pkill -RTMIN+15 $BAR
Print
	screeenshot.sh
